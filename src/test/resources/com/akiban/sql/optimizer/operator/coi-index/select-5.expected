PhysicalSelect[name:varchar(32)[VARCHAR], order_date:date[DATE]]
  project([Field(1), Field(4)])
    Flatten_HKeyOrdered(user.customers INNER user.orders)
      AncestorLookup_Default(Index(user.orders.order_date[IndexColumn(order_date)]) -> [user.customers, user.orders])
        IndexScan_Default(Index(user.orders.order_date[IndexColumn(order_date)]) (,) (Field(0) DESC))