{

"setup" : {
    "processors_default" : ["scanrows"],

    "schema" : "test",

    "tables" : [
        ["years", "id int not null primary key", "y year", "index(y)"]
    ],

    "write_rows" : {
        "years"  : [
            [1, "2011"],
            [2, "1901"],
            [3, "0"],
            [4, "2155"]
        ]
    }
},

"tests" : {
    "select_all" : {
        "get" : "test:years:id>0",
        "expect result" : {
            "@years" : [
                {"id" : 1, "y" : "2011"},
                {"id" : 2, "y" : "1901"},
                {"id" : 3, "y" : "0"},
                {"id" : 4, "y" : "2155"}
            ]
        }
    },

    "index_less" : {
        "get" : "test:years:y<'2000'",
        "expect result" : {
            "@years" : [
                {"id" : 3, "y" : "0"},
                {"id" : 2, "y" : "1901"}
            ]
        }
    },

    "index_equal" : {
        "get" : "test:years:y='1901'",
        "expect result" : {
            "@years" : [
                {"id" : 2, "y" : "1901"}
            ]
        }
    },

    "index_greater" : {
        "get" : "test:years:y>'1950'",
        "expect result" : {
            "@years" : [
                {"id" : 1, "y" : "2011"},
                {"id" : 4, "y" : "2155"}
            ]
        }
    },
}

}
