{

"setup" : {
    "processors_default" : ["scanrows"],

    "schema" : "test",

    "tables" : [
        ["times", "id int not null primary key", "i int", "t timestamp", "index(t)"]
    ],

    "write_rows" : {
        "times"  : [
            [1, 1000000000, "2001-09-09 04:46:40"],
            [2,          0, "0000-00-00 00:00:00"],
            [3, 1111111111, "2005-03-18 01:58:31"],
            [4, 1342177280, "2012-07-13 11:01:20"],
            [5, 2034061609, "2034-06-16 09:06:49"],
            [6,      45678, "1970-01-01 12:41:18"],
            [7,  987654321, "2001-04-19 04:25:21"]
        ]
    }
},

"tests" : {
    "select_all" : {
        "get" : "test:times:id>0",
        "expect result" : {
            "@times" : [
                {"id" : 1, "i" : 1000000000, "t" : "2001-09-09 04:46:40"},
                {"id" : 2, "i" :          0, "t" : "0000-00-00 00:00:00"},
                {"id" : 3, "i" : 1111111111, "t" : "2005-03-18 01:58:31"},
                {"id" : 4, "i" : 1342177280, "t" : "2012-07-13 11:01:20"},
                {"id" : 5, "i" : 2034061609, "t" : "2034-06-16 09:06:49"},
                {"id" : 6, "i" :      45678, "t" : "1970-01-01 12:41:18"},
                {"id" : 7, "i" :  987654321, "t" : "2001-04-19 04:25:21"}
            ]
        }
    },

    "index_less" : {
        "get" : "test:times:t<'1973-10-20%2021%3A20%3A00'",
        "expect result" : {
            "@times" : [
                {"id" : 2, "i" :     0, "t" : "0000-00-00 00:00:00"},
                {"id" : 6, "i" : 45678, "t" : "1970-01-01 12:41:18"}
            ]
        }
    },

    "index_equal" : {
        "get" : "test:times:t='2005-03-18%2001%3A58%3A31'",
        "expect result" : {
            "@times" : [
                {"id" : 3, "i" : 1111111111, "t" : "2005-03-18 01:58:31"},
            ]
        }
    },

    "index_greater" : {
        "get" : "test:times:t>'1986-10-28%2000%3A00%3A00'",
        "expect result" : {
            "@times" : [
                {"id" : 7, "i" :  987654321, "t" : "2001-04-19 04:25:21"},
                {"id" : 1, "i" : 1000000000, "t" : "2001-09-09 04:46:40"},
                {"id" : 3, "i" : 1111111111, "t" : "2005-03-18 01:58:31"},
                {"id" : 4, "i" : 1342177280, "t" : "2012-07-13 11:01:20"},
                {"id" : 5, "i" : 2034061609, "t" : "2034-06-16 09:06:49"}
            ]
        }
    }
}

}
