{

"setup" : {
    "processors_default" : ["scanrows"],

    "schema" : "test",

    "tables" : [
        ["times", "id int not null primary key", "t time", "index(t)"]
    ],

    "write_rows" : {
        "times"  : [
            [1, "17:06:59"],
            [2, "01:01:42"],
            [3, "00:00:00"],
            [4, "838:59:59"],
            [5, "-10:00:00"],
            [6, "-36:15:23"],
            [7, "00:00:05"]
        ]
    }
},

"tests" : {
    "select_all" : {
        "get" : "test:times:id>0",
        "expect result" : {
            "@times" : [
                {"id" : 1, "t" : "17:06:59"},
                {"id" : 2, "t" : "01:01:42"},
                {"id" : 3, "t" : "00:00:00"},
                {"id" : 4, "t" : "838:59:59"},
                {"id" : 5, "t" : "-10:00:00"},
                {"id" : 6, "t" : "-36:15:23"},
                {"id" : 7, "t" : "00:00:05"}
            ]
        }
    },

    "index_less_sec" : {
        "get" : "test:times:t<'6'",
        "expect result" : {
            "@times" : [
                {"id" : 6, "t" : "-36:15:23"},
                {"id" : 5, "t" : "-10:00:00"},
                {"id" : 3, "t" : "00:00:00"},
                {"id" : 7, "t" : "00:00:05"}
            ]
        }
    },

    "index_less_min_sec" : {
        "get" : "test:times:t<'-05%3A10'",
        "expect result" : {
            "@times" : [
                {"id" : 6, "t" : "-36:15:23"},
                {"id" : 5, "t" : "-10:00:00"}
            ]
        }
    },

    "index_less_full" : {
        "get" : "test:times:t<'-10%3A00%3A01'",
        "expect result" : {
            "@times" : [
                {"id" : 6, "t" : "-36:15:23"}
            ]
        }
    },

    "index_equal_sec" : {
        "get" : "test:times:t='05'",
        "expect result" : {
            "@times" : [
                {"id" : 7, "t" : "00:00:05"}
            ]
        }
    },

    "index_equal_full" : {
        "get" : "test:times:t='-36%3A15%3A23'",
        "expect result" : {
            "@times" : [
                {"id" : 6, "t" : "-36:15:23"}
            ]
        }
    },

    "index_greater_min_sec" : {
        "get" : "test:times:t>'05%3A10'",
        "expect result" : {
            "@times" : [
                {"id" : 2, "t" : "01:01:42"},
                {"id" : 1, "t" : "17:06:59"},
                {"id" : 4, "t" : "838:59:59"}
            ]
        }
    },

    "index_greater_full" : {
        "get" : "test:times:t>'00%3A00%3A00'",
        "expect result" : {
            "@times" : [
                {"id" : 7, "t" : "00:00:05"},
                {"id" : 2, "t" : "01:01:42"},
                {"id" : 1, "t" : "17:06:59"},
                {"id" : 4, "t" : "838:59:59"}
            ]
        }
    }
}

}
