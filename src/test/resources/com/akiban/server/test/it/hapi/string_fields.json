{

"setup" : {
    "processors_default" : ["scanrows"],

    "schema" : "test",

    "tables" : [
        ["strings", "id int not null primary key", "c char(10)", "v varchar(10)", "index(c)", "index(v)"]
    ],

    "write_rows" : {
        "strings"  : [
            [1, "1234567890", "a"],
            [2, "a", ""],
            [3, "hello", "1234567890"],
            [4, "zebra", "banana"],
            [5, "banana", "'quoted'"],
            [6, "", "hello"],
            [7, "'quoted'", "zebra"]
        ]
    }
},

"tests" : {
    "select_all" : {
        "get" : "test:strings:id>0",
        "expect result" : {
            "@strings" : [
                {"id" : 1, "c" : "1234567890", "v" : "a"},
                {"id" : 2, "c" : "a", "v" : ""},
                {"id" : 3, "c" : "hello", "v" : "1234567890"},
                {"id" : 4, "c" : "zebra", "v" : "banana"},
                {"id" : 5, "c" : "banana", "v" : "'quoted'"},
                {"id" : 6, "c" : "", "v" : "hello"},
                {"id" : 7, "c" : "'quoted'", "v" : "zebra"}
            ]
        }
    },

    "index_equal_char" : {
        "get" : "test:strings:c=''",
        "expect result" : {
            "@strings" : [
                {"id" : 6, "c" : "", "v" : "hello"},
            ]
        }
    },

    "index_greater_char" : {
        "get" : "test:strings:c>''",
        "expect result" : {
            "@strings" : [
                {"id" : 7, "c" : "'quoted'", "v" : "zebra"},
                {"id" : 1, "c" : "1234567890", "v" : "a"},
                {"id" : 2, "c" : "a", "v" : ""},
                {"id" : 5, "c" : "banana", "v" : "'quoted'"},
                {"id" : 3, "c" : "hello", "v" : "1234567890"},
                {"id" : 4, "c" : "zebra", "v" : "banana"}
            ]
        }
    },

    "index_equal_varchar" : {
        "get" : "test:strings:v=''",
        "expect result" : {
            "@strings" : [
                {"id" : 2, "c" : "a", "v" : ""},
            ]
        }
    },

    "index_greater_varchar" : {
        "get" : "test:strings:v>''",
        "expect result" : {
            "@strings" : [
                {"id" : 5, "c" : "banana", "v" : "'quoted'"},
                {"id" : 3, "c" : "hello", "v" : "1234567890"},
                {"id" : 1, "c" : "1234567890", "v" : "a"},
                {"id" : 4, "c" : "zebra", "v" : "banana"},
                {"id" : 6, "c" : "", "v" : "hello"},
                {"id" : 7, "c" : "'quoted'", "v" : "zebra"}
            ]
        }
    }
}

}
